datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id            Int     @id @default(autoincrement())
  name          String
  email         String  @unique
  accountNumber String  @unique
  balance       Float   @default(0)
  password      String
}
